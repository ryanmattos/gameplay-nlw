{
  "expo": {
    "name": "gameplay",
    "slug": "gameplay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "gameplay",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0d133e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ryanmatos.gameplay"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A Mobile app that allows you to schedule your game plays with your friends from discord",
    "githubUrl": "https://github.com/ryanmattos/gameplay-nlw"
  }
}
